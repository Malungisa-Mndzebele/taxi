{
  "name": "taxi-client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "web": "expo start --web",
    "web:start": "webpack serve --mode development --config webpack.config.js --hot --open",
    "web:build": "webpack --mode production --config webpack.config.js",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace TaxiApp.xcworkspace -scheme TaxiApp -configuration Release -destination generic/platform=iOS -archivePath TaxiApp.xcarchive archive"
  },
